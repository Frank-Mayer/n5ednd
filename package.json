    "sass": "^1.56.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@frank-mayer/magic": "^4.0.2",
    "@frank-mayer/service-worker": "^1.2.3",
    "idb": "^7.1.0",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.9.1",
    "react-dom": "^18.2.0",
    "react-numeric-input": "^2.2.3"
  }
}
